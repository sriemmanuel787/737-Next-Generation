<?xml version="1.0"?>

<PropertyList>
    <path>efis-control-panel.ac</path>

    <animation>
        <type>knob</type>
        <object-name>Range.knob</object-name>
        <property>instrumentation/efis[1]/inputs/range-knob</property>
        <factor>30</factor>
        <offset-deg>-5</offset-deg>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0128</y-m>
            <z-m>0.0128</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <increase>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"RANGE",1);</script>
            </binding>
        </increase>
        <decrease>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"RANGE",-1);</script>
            </binding>
        </decrease>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>MODE.knob</object-name>
        <property>instrumentation/efis[1]/mfd/mode-num</property>
        <factor>30</factor>
        <offset-deg>-90</offset-deg>
        <center>
            <x-m>0.0</x-m>
            <y-m>-0.01999</y-m>
            <z-m>0.01252</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <increase>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"MODE",1);</script>
            </binding>
        </increase>
        <decrease>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"MODE",-1);</script>
            </binding>
        </decrease>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>ND.center</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
            <command>property-toggle</command>
            <property>instrumentation/efis[1]/inputs/nd-centered</property>
        </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>BARO.knob2</object-name>
        <property>instrumentation/efis[1]/inputs/baro-knob</property>
        <factor>5</factor>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.03414</y-m>
            <z-m>0.03456</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/efis[1]/inputs/baro-knob</property>
            </binding>
        </action>
        <increase>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"BARO",1);</script>
            </binding>
        </increase>
        <decrease>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"BARO",-1);</script>
            </binding>
        </decrease>
        <shift-increase>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"BAROUNIT",1);</script>
            </binding>
        </shift-increase>
        <shift-decrease>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"BAROUNIT",-1);</script>
            </binding>
        </shift-decrease>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>inhg.reset</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(1,"STD",0);</script>
            </binding>
        </action>
    </animation>

