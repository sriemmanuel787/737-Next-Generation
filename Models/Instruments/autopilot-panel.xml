    <animation>
    	<type>rotate</type>
    	<object-name>BANK</object-name>
		<property>it-autoflight/input/bank-limit-sw</property>
		<interpolation>
			<entry><ind>2</ind><dep>185</dep></entry>
			<entry><ind>6</ind><dep>70</dep></entry>
		</interpolation>     
    	<center>
    		<x-m>0.033</x-m>
    		<y-m>-0.047</y-m>
